        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "projects/ng-fire-brewerbase/tsconfig.app.json",
              "projects/ng-fire-brewerbase/tsconfig.spec.json"
            ],
            "exclude": [
              "**/node_modules/**"
            ]
          }
        }
      }
    },
    "ng-fire-brewerbase-e2e": {
      "root": "projects/ng-fire-brewerbase-e2e/",
      "projectType": "application",
      "architect": {
        "e2e": {
          "builder": "@angular-devkit/build-angular:protractor",
          "options": {
            "protractorConfig": "projects/ng-fire-brewerbase-e2e/protractor.conf.js",
